RewriteEngine on
RewriteCond %{REQUEST_FILENAME} !-d
RewriteCond %{REQUEST_FILENAME} !-f

# change this to point to hosting URL
# ex: /index.php?page=404
ErrorDocument 404 /index.php?page=404

RewriteRule about/ index.php?page=about
RewriteRule directions/ index.php?page=directions
RewriteRule what-to-bring/ index.php?page=bring
RewriteRule visit/ index.php?page=form

RewriteRule admin/ index.php?page=admin
